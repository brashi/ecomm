{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "plugins": ["jest"],
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-underscore-dangle": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
         ]
    }
}
